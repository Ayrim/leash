<div class="col s12">
	<div class="row">
		<% if !@albumphotos.nil? %>
			<% @albumphotos.each do |photo| %>
				<%= link_to "#showImage", :class => "modal-trigger" do %>                
					<div class="" style="float: left; display: inline-table; width: 33%; margin-right:0.3%; margin-bottom:0.3%">
						<div class="card-image waves-effect waves-block waves-light imageInAlbum" style="background:url(<%= photo.url %>) no-repeat center center; width:auto; background-size: cover; margin:0.6%">
						</div>
					</div>
				<% end %>
			<% end %>
		<% end %>
	</div>
</div>