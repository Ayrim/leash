<%= stylesheet_link_tag "application", params[:controller], :media => "all" %>
<%= stylesheet_link_tag "fullcalendar", :media => "all" %>


<div class="container">

  <div class="row">
    <% @dog.each do |dog| %>
    <div class="col s12 m6">


      <ul class="collection z-depth-1 with-header hoverable card">
        <li class="collection-header"><strong><%= dog.animal.name %></strong></li>
        <%= link_to 'Show', dog_path(dog) %>
        <img src="https://pbs.twimg.com/profile_images/1846296430/ProfielFoto_400x400.png" alt="" class="circle">



        <li class="collection-item avatar">

          <span class="title truncate">Maxim Braekman</span>
          <p class="truncate">Blankenberge <br>
            Distance: 0.00 km
          </p>
        </li>


      </ul>
    </div>
    <% end %>



  </div>
</div>


<script type="text/javascript">

  $(document).ready(function(){
    // page is now ready, initialize the calendar...
    $('#calendar').fullCalendar({
      // put your options and callbacks here
      header: {
        left: 'prev,next today',
        center: 'title',
        right: 'month,agendaWeek,agendaDay'
      },
      defaultView: 'month',
      editable: true,
      events: [
        {
          title: 'Deadline',
          start: '2016-04-01'
        },
        {
          id: 999,
          title: 'Repeating Event',
          start: '2016-04-09T16:00:00'
        },
        {
          id: 999,
          title: 'Repeating Event',
          start: '2016-04-16T16:00:00'
        },
        {
          title: 'Meeting',
          start: '2016-04-12T10:30:00',
          end: '2016-04-12T12:30:00'
        },
        {
          title: 'Lunch',
          start: '2016-04-12T12:00:00'
        }]
    })
  })
</script>












<h1>Dogs</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th colspan="2"></th>
  </tr>

  <% @dog.each do |dog| %>
      <tr>
        <td><%= dog.animal.name %></td>
        <td><%= dog.breed %></td>
        <td><%= link_to 'Show', dog_path(dog) %></td>
        <td><%= link_to 'Edit', edit_dog_path(dog) %></td>
      </tr>
  <% end %>
</table>